{
  "name": "@times-components/fructose",
  "version": "2.2.4",
  "main": "fructose.js",
  "scripts": {
    "test:unit": "jest --verbose --forceExit",
    "test": "./scripts/tests.sh",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "lint-staged": "lint-staged",
    "lint:packages": "eslint . --fix",
    "lint:fructose": "eslint e2eTests/.fructose /--fix",
    "lint:e2eTests": "eslint e2eTests /--fix",
    "prettier:diff": "prettier --list-different '**/*.js'",
    "prettier:write": "prettier --write '**/*.js'",
    "prepare": "babel packages/test-helpers/src --out-dir packages/test-helpers/lib"
  },
  "bin": {
    "compile-tests": "packages/test-helpers/bin/writeComponentsTests.js",
    "fructose-web": "packages/web/bin/start.js"
  },
  "pre-commit": [
    "precommit-msg",
    "lint-staged",
    "test:unit"
  ],
  "lint-staged": {
    "**/*.js": [
      "lint:packages",
      "lint:fructose",
      "lint:e2eTests",
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "preset": "react-native",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/e2eTests",
      "<rootDir>/packages/test-helpers/lib"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "blink-diff": "^1.0.13",
    "caller-id": "^0.1.0",
    "callsite": "^1.0.0",
    "clipboardy": "^1.1.4",
    "commander": "^2.11.0",
    "console-png": "^1.2.1",
    "copy-paste": "^1.3.0",
    "detox":"5.8.1",
    "events": "1.1.1",
    "express": "^4.15.3",
    "global": "4.3.2",
    "html-webpack-plugin": "^2.28.0",
    "jimp": "^0.2.28",
    "looks-same": "^3.2.1",
    "npmlog": "^4.1.2",
    "osnap": "^1.1.0",
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.3.0",
    "prop-types": "15.5.10",
    "request": "^2.82.0",
    "server-destroy": "^1.0.1",
    "socket.io": "2.0.3",
    "socket.io-client": "2.0.3",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-merge": "^4.1.0"
  },
  "peerDependencies": {
    "react": "16.0.0-alpha.6",
    "react-native": "0.44.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-react-native": "2.0.0",
    "babel-register": "6.24.1",
    "enzyme": "2.8.2",
    "enzyme-to-json": "1.5.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-config-prettier": "2.3.0",
    "eslint-plugin-import": "2.6.1",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "jest": "20.0.4",
    "jest-environment-node-debug": "2.0.0",
    "lerna": "^2.0.0-rc.5",
    "lint-staged": "4.0.0",
    "nyc": "^11.0.3",
    "portfinder": "^1.0.13",
    "pre-commit": "1.2.2",
    "prettier": "1.4.2",
    "react": "16.0.0-alpha.6",
    "react-addons-test-utils": "15.6.0",
    "react-dom": "15.6.1",
    "react-native": "0.44.1",
    "react-native-cli": "2.0.1",
    "sinon": "2.3.6",
    "tap-nyc": "^1.0.2",
    "tap-summary": "^3.0.2",
    "tape": "^4.7.0"
  },
  "author": "News UK & Ireland Ltd",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/rjanjua/fructose/issues"
  }
}
